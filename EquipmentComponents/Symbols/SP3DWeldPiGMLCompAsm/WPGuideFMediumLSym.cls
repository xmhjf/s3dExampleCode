Option Explicit

Private Const MODULE = "WPGuideFMediumLSym:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices


Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DWeldPiGMLCompAsm"
    m_oSymbolHelper.ClassName = "WPGuideFMediumLSym"
    
' Inputs
    m_oSymbolHelper.NumInputs = 4
    m_oSymbolHelper.AddInputDef 1, "VesselDiameter", "Tower Diameter", 0.9
    m_oSymbolHelper.AddInputDef 2, "PipeCLtoTowerCL", "Pipe CenterLine to Tower CenterLine-Y", 1.82
    m_oSymbolHelper.AddInputDef 3, "TowertoGuideBase", "Tower Outer shell to Guide Base-K", 0.6
    m_oSymbolHelper.AddInputDef 4, "PipeOD", "Pipe Outer Diameter", 0.6

' Outputs
    m_oSymbolHelper.NumOutputs = 20
    m_oSymbolHelper.AddOutputDef 1, "Saddle", "Saddle Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "SaddleSupport1", "Top Trapizoidal Saddle Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "SaddleSupport2", "Bottom Trapizoidal Saddle Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "SaddleSupport3", "Box Type portion fits in Clamps", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "SupportCenterPlate1", "Guide saddle support CenterPlate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "GuideSupport1", "Tower circular support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "GuideSupport2", "Top guide plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "GuideSupport3", "Guide Clamp Plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "SupportCenterPlate2", "Guide support CenterPlate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "ZClamp1A", "Left Z-Clamp Inner Box", SimplePhysical
    m_oSymbolHelper.AddOutputDef 11, "ZClamp1B", "Left Z-Clamp Outer Box", SimplePhysical
    m_oSymbolHelper.AddOutputDef 12, "ZClamp2A", "Right Z-Clamp Inner Box", SimplePhysical
    m_oSymbolHelper.AddOutputDef 13, "ZClamp2B", "Right Z-Clamp Outer Box", SimplePhysical
    m_oSymbolHelper.AddOutputDef 14, "Bolt1", "Left Bolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 15, "Bolt2", "Right Bolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 16, "DefaultSurface", "Default Surface Reference plane on Tower", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 17, "VerticalPipeLine", "Reference line on Pipe", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 18, "RadialTowerLine", "Reference line on Tower", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 19, "TowerPoint", "Point at origin", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 20, "PipePoint", "Point at Pipe Axis", ReferenceGeometry
    

' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry
    
    Exit Sub
    
ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

