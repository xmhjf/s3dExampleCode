Option Explicit

Private Const MODULE = "CFXACDNSym:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()

    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DFrEnXACDNCompAsm"
    m_oSymbolHelper.ClassName = "CFXACDNSym"

' Inputs
    m_oSymbolHelper.NumInputs = 8

    m_oSymbolHelper.AddInputDef 1, "FrontEndFlangeDia", "Front End Flange Diameter P30", 0.55
    m_oSymbolHelper.AddInputDef 2, "FrontEndFlangeTk1", "Front End Flange Thickness 1 P31", 0.065
    m_oSymbolHelper.AddInputDef 3, "FrontEndLength1", "Front End Length1  P32", 0.75
    m_oSymbolHelper.AddInputDef 4, "FrontEndLength2", "Front End Length2  P33", 0.3
    m_oSymbolHelper.AddInputDef 5, "FrontEndFlangeTk2", "Front End Flange Thickness 2 P34", 0.05
    m_oSymbolHelper.AddInputDef 6, "FrontEndFlangeTk3", "Front End Flange Thickness 3 P35", 0.065
    m_oSymbolHelper.AddInputDef 7, "ChannelDiameter", "Channel Diameter", 0.2
    m_oSymbolHelper.AddInputDef 8, "InsulationThickness", "Insulation Thickness", 0.025
    
' Outputs
    m_oSymbolHelper.NumOutputs = 10
    m_oSymbolHelper.AddOutputDef 1, "ExchFrontEndRHSFlange", "Exchanger Front End Right Hand Side Flange", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "ExFrontEndBody", "Exchanger Front End Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "ExFrontEndBodyIFlange", "Exchanger Front End Body Intermediate Flange", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "ExchFrontEndLHSFlange", "Exchanger Front End Left Hand Side Flange", SimplePhysical
    
    m_oSymbolHelper.AddOutputDef 5, "ExchFrontEndRHSFlangeIns", "Exchanger Front End Right Hand Side FlangeInsulation", Insulation
    m_oSymbolHelper.AddOutputDef 6, "ExFrontEndBodyIns", "Exchanger Front End Body Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 7, "ExFrontEndBodyIFlangeIns", "Exchanger Front End Body Intermediate Flange Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 8, "ExchFrontEndLHSFlangeIns", "Exchanger Front End Left Hand Side Flange Insulation", Insulation
    
    m_oSymbolHelper.AddOutputDef 9, "FrEndExchTypeACDNControlPoint", "Control Point", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 10, "DefaultSurface", "Default Surface", ReferenceGeometry

' Aspects
    m_oSymbolHelper.NumAspects = 3
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    m_oSymbolHelper.AddAspectDef 3, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry

    Exit Sub

ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

