Option Explicit

Private Const MODULE = "CVSkirtSym:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()

    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DVesselSkirtAsm"
    m_oSymbolHelper.ClassName = "CVSkirtSym"

' Inputs
    m_oSymbolHelper.NumInputs = 8
    m_oSymbolHelper.AddInputDef 1, "VesselDiameter", "Vessel Diameter", 2.2
    m_oSymbolHelper.AddInputDef 2, "SkirtBottomDiameter", "Skirt Bottom Diameter", 2.8
    m_oSymbolHelper.AddInputDef 3, "SkirtHeight", "Skirt Height", 4
    m_oSymbolHelper.AddInputDef 4, "SupportHeight", "Support Height", 0.4
    m_oSymbolHelper.AddInputDef 5, "SupportDiameter", "Support Diameter", 3.2
    m_oSymbolHelper.AddInputDef 6, "BasePlateThickness", "Base Plate Thickness", 0.01
    m_oSymbolHelper.AddInputDef 7, "BasePlateDiameter", "'BasePlate Diameter' ", 2.95
    m_oSymbolHelper.AddInputDef 8, "InsulationThickness", "Insulation Thickness", 0.025

' Outputs
    m_oSymbolHelper.NumOutputs = 10
    m_oSymbolHelper.AddOutputDef 1, "Skirt", "Skirt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "BasePlate", "Base Plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Gusset", "Gusset", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "AnchorPlate", "Anchor Plate", SimplePhysical

    'Insulation Aspect
    m_oSymbolHelper.AddOutputDef 5, "SupportIns", "Support Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 6, "SkirtIns", "Skirt Insulation", Insulation

    'Refernce Geometry Outputs
    m_oSymbolHelper.AddOutputDef 7, "OriginPoint", "Point at origin", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 8, "VerticalLine", "Vertical Line", ReferenceGeometry 'Ask Ravi Line is vertical / horizontal
    m_oSymbolHelper.AddOutputDef 9, "DefaultSurface", "Default Surface at the Top", ReferenceGeometry 'Ask Ravi Line is vertical / horizontal
    m_oSymbolHelper.AddOutputDef 10, "BotDefaultSurface", "Default Surface at the Bottom", ReferenceGeometry 'Ask Ravi Line is vertical / horizontal

' Aspects
    m_oSymbolHelper.NumAspects = 3
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    m_oSymbolHelper.AddAspectDef 3, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry

    Exit Sub

ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

