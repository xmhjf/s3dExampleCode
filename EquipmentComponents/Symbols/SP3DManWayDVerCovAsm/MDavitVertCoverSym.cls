Option Explicit

Private Const MODULE = "MDavitVertCoverSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()

    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DManWayDVerCovAsm"
    m_oSymbolHelper.ClassName = "MDavitVertCoverSym"

' Inputs
    m_oSymbolHelper.NumInputs = 4
    m_oSymbolHelper.AddInputDef 1, "CentertoDavitCover", "Outer Face of manhole cover to center of Vessel", 2.5
    m_oSymbolHelper.AddInputDef 2, "VesselDiameter", "Tower Vessel Outer Diameter", 4
    m_oSymbolHelper.AddInputDef 3, "DavitHinge", "Davit Hinge Position", 3
    m_oSymbolHelper.AddInputDef 4, "InsulationThickness", "Insulation Thickness", 0.025

    
' Outputs
    m_oSymbolHelper.NumOutputs = 16
    m_oSymbolHelper.AddOutputDef 1, "Manhole", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "ManholeCover", "ManHole Cover", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "EyeBolt", "Eye Bolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "HexNut", "Hexagonal Nut for EyeBolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "DavitArmTop", "Davit Arm Top", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "DavitArmCurved", "Davit Arm Curved", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "DavitArmCollar", "Davit Arm Collar", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "DavitArmBot", "Davit Arm Bottom", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "HingePlate", "Hinge Plate", SimplePhysical

    'Maintenance Aspect
    m_oSymbolHelper.AddOutputDef 10, "MaintenaceEnvelope", "Maintenace Envelope", Maintenance

    'Refernce Geometry Outputs
    m_oSymbolHelper.AddOutputDef 11, "OriginPoint", "Point at origin", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 12, "VesselRadialLine", "Vessel Radial Line", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 13, "VesselAxisLine", "Vessel Axial Line", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 14, "DefaultSurface", "Default Surface", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 15, "RefSurface", "Reference Surface", ReferenceGeometry

    'Insulation Aspect
    m_oSymbolHelper.AddOutputDef 16, "NozzleIns", "Insulation", Insulation

' Aspects
    m_oSymbolHelper.NumAspects = 4
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    m_oSymbolHelper.AddAspectDef 3, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry
    m_oSymbolHelper.AddAspectDef 4, "Maintenance", "Maintenance", Maintenance

    Exit Sub

ErrorHandler:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

