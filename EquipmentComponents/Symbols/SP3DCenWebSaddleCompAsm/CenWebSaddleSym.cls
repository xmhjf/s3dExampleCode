Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DCenWebSaddleCompAsm"
    m_oSymbolHelper.ClassName = "CenWebSym"

    'Inputs
    m_oSymbolHelper.NumInputs = 13
    
    m_oSymbolHelper.AddInputDef 1, "VesselDiameter", "Vessel Diameter", 0.6906
    m_oSymbolHelper.AddInputDef 2, "BasePlateLength", "A-Baseplate Length", 0.5588
    m_oSymbolHelper.AddInputDef 3, "CenToSaddleBottom", "B-Center to Saddle Bottom", 0.6096
    m_oSymbolHelper.AddInputDef 4, "BackingPlateThick", "C-Backing Plate Thickness", 0.00635
    m_oSymbolHelper.AddInputDef 5, "BasePlateThick", "D-Baseplate Thickness", 0.0095
    m_oSymbolHelper.AddInputDef 6, "XBoltHole", "E-Hole Bolt X Location", 0.1524
    m_oSymbolHelper.AddInputDef 7, "BasePlateWidth", "F-Baseplate Width", 0.2286
    m_oSymbolHelper.AddInputDef 8, "RibWidth", "G-Rib Width", 0.2032
    m_oSymbolHelper.AddInputDef 9, "BackingPlateWidth", "H-Backing Plate Width", 0.254
    m_oSymbolHelper.AddInputDef 10, "WebThickness", "J-Web Thickness", 0.00635
    m_oSymbolHelper.AddInputDef 11, "RibThick", "L-Rib Thickness", 0
    m_oSymbolHelper.AddInputDef 12, "YBoltHole", "M-Hole Bolt Y Location", 0.01905
    m_oSymbolHelper.AddInputDef 13, "NoOfRibs", "Number of Ribs", 0
    
    'Outputs
    m_oSymbolHelper.NumOutputs = 6
    
    m_oSymbolHelper.AddOutputDef 1, "BackingPlate", "Backing Plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Web", "Web", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Rib_", "Rib", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Baseplate", "Baseplate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "EqpFoundationPort", "Foundation Port under support", SimplePhysical
    
    'Aspects
    m_oSymbolHelper.NumAspects = 1
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
