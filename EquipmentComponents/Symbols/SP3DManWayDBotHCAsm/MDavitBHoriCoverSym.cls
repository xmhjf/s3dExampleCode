Option Explicit

Private Const MODULE = "MDavitBHoriCoverSym:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DManWayDBotHCAsm"
    m_oSymbolHelper.ClassName = "MDavitBHoriCoverSym"
    
' Inputs
    m_oSymbolHelper.NumInputs = 3
    m_oSymbolHelper.AddInputDef 1, "CentertoDavitCover", "Nozzle Position", 0.7
    m_oSymbolHelper.AddInputDef 2, "DomeHeight1", "Dome Height 1", 0.5
    m_oSymbolHelper.AddInputDef 3, "InsulationThickness", "Insulation Thickness", 0.025
    
' Outputs
    m_oSymbolHelper.NumOutputs = 16
    m_oSymbolHelper.AddOutputDef 1, "Manhole", "Nozzle with Length", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "CoverBody", "Blind Flange", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "BotSupPlate", "Bottom Support Plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "BotDavitSup", "Channel Plate attached to bottom flange", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "TopDavitSup1", "Vertical Plate attached to Top Flange", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "TopDavitSup2", "Vertical Plate attached to Top Flange", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "DavitPin", "Davit Pin", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "HexNut", "Hex Nut at the bottom of davit", SimplePhysical

    'Refernce Geometry Outputs
    m_oSymbolHelper.AddOutputDef 9, "OriginPoint", "Point at origin", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 10, "DefaultSurface", "Default Surface", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 11, "RefLine", "Reference Line", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 12, "PipeAxisLine", "Pipe Axis Line", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 13, "VesselAxisPoint", "Vessel Axis Point", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 14, "RefSurface", "Reference Surface", ReferenceGeometry
    
    'Maintenance Aspect
    m_oSymbolHelper.AddOutputDef 15, "MaintEnv", "Maintenance Envelope", Maintenance
    
    'Insulation Aspect
    m_oSymbolHelper.AddOutputDef 16, "NozzleIns", "Insulation", Insulation
    
' Aspects
    m_oSymbolHelper.NumAspects = 4
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry
    m_oSymbolHelper.AddAspectDef 3, "Maintenance", "Maintenance", Maintenance
    m_oSymbolHelper.AddAspectDef 4, "Insulation", "Insulation", Insulation
    
    Exit Sub
    
ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

