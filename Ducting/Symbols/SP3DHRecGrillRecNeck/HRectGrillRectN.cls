Option Explicit

Private Const MODULE = "HRectGrillRectN:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
Private Const E_FAIL = &H80004005
Private Const lngNMaxOutputs = 40
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
     
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DHRecGrillRecNeck"
    m_oSymbolHelper.ClassName = "HRectGrillRectN"
    
' Inputs
    m_oSymbolHelper.NumInputs = 8
    
    m_oSymbolHelper.AddInputDef 1, "Width", "", 0.025
    m_oSymbolHelper.AddInputDef 2, "Depth", "", 0.025
    m_oSymbolHelper.AddInputDef 3, "GrillBladeWidth", "", 0.025
    m_oSymbolHelper.AddInputDef 4, "FrameWidth", "", 0.05
    m_oSymbolHelper.AddInputDef 5, "GrillWidth", "", 0.005
    m_oSymbolHelper.AddInputDef 6, "GrillHeight", "", 0.125
    m_oSymbolHelper.AddInputDef 7, "GrillLength", "", 0.225
    m_oSymbolHelper.AddInputDef 8, "NeckLength", "", 0.225
    
' Outputs
    m_oSymbolHelper.NumOutputs = lngNMaxOutputs
 
    m_oSymbolHelper.AddOutputDef 1, "HvacNozzle1", "HvacNozzle1", 1
    m_oSymbolHelper.AddOutputDef 2, "Neck", "Neck", 1
    m_oSymbolHelper.AddOutputDef 3, "ConePortion", "ConePortion", 1
    
    Dim i As Long
    
    For i = 4 To lngNMaxOutputs
        m_oSymbolHelper.AddOutputDef i, "Feature" & i, "Feature" & i, 1
    Next i
 
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
    Exit Sub
    
End Sub
Private Sub Class_Terminate()
   
    Set m_oSymbolHelper = Nothing

End Sub

