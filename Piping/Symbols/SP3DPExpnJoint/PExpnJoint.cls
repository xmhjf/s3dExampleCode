'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DPExpnJoint"
    m_oSymbolHelper.ClassName = "PExpnJoint"

'**********************************************************************************
'**********************************************************************************
'   BEGIN
'**********************************************************************************
'**********************************************************************************

        m_oSymbolHelper.NumInputs = 8
        m_oSymbolHelper.AddInputDef 1, "FacetoCenter", "FacetoCenter", 1
        m_oSymbolHelper.AddInputDef 2, "Face1toCenter", "Face1toCenter", 2
        m_oSymbolHelper.AddInputDef 3, "PipingSpecialityLength", "Piping Speciality Length", 0.5
        m_oSymbolHelper.AddInputDef 4, "PipingSpecialityLength1", "Piping Speciality Length 1", 0.65
        m_oSymbolHelper.AddInputDef 5, "PipingSpecialityLength2", "Piping Speciality Length 2", 0.2
        m_oSymbolHelper.AddInputDef 6, "CylinderDiameter", "Cylinder Diameter", 0.8
        m_oSymbolHelper.AddInputDef 7, "PipingSpecialityLength3", "Piping Speciality Length 3", 0.3
        m_oSymbolHelper.AddInputDef 8, "CylinderDiameter1", "Cylinder Diameter 1", 0.7
    
        m_oSymbolHelper.NumOutputs = 8
'        m_oSymbolHelper.AddOutputDef 1, "oCylinderCapped1", "oCylinderCapped1", 1
        m_oSymbolHelper.AddOutputDef 1, "oCylinderCapped2", "oCylinderCapped2", 1
        m_oSymbolHelper.AddOutputDef 2, "oCylinderCapped3", "oCylinderCapped3", 1
        m_oSymbolHelper.AddOutputDef 3, "oCylinderCapped4", "oCylinderCapped4", 1
        m_oSymbolHelper.AddOutputDef 4, "oCylinderCapped5", "oCylinderCapped5", 1
        m_oSymbolHelper.AddOutputDef 5, "oCylinderCapped6", "oCylinderCapped6", 1
        m_oSymbolHelper.AddOutputDef 6, "oCylinderCapped7", "oCylinderCapped7", 1
'        m_oSymbolHelper.AddOutputDef 8, "oCylinderCapped8", "oCylinderCapped8", 1
        m_oSymbolHelper.AddOutputDef 7, "Nozzle1", "Nozzle1", 1
        m_oSymbolHelper.AddOutputDef 8, "Nozzle2", "Nozzle2", 1
    
'**********************************************************************************
'**********************************************************************************
'   END
'**********************************************************************************
'**********************************************************************************
    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "SimplePhysicalAspect Description", 1
    
    Exit Sub
    
ErrorHandler:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

