'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DP2WayPlugCtrlVal"
    m_oSymbolHelper.ClassName = "P2WayPlugCtrlVal"


'**********************************************************************************
'**********************************************************************************
'   BEGIN
'**********************************************************************************
'**********************************************************************************

        m_oSymbolHelper.NumInputs = 2
        m_oSymbolHelper.AddInputDef 1, "FacetoCenter", "FacetoCenter", 1
        m_oSymbolHelper.AddInputDef 2, "Face1toCenter", "Face1toCenter", 2
    
        m_oSymbolHelper.NumOutputs = 6
'       m_oSymbolHelper.AddOutputDef 1, "oCylinderCapped1", "oCylinderCapped1", 1
        m_oSymbolHelper.AddOutputDef 1, "oCone1", "oCone1", 1
        m_oSymbolHelper.AddOutputDef 2, "oCone2", "oCone2", 1
'       m_oSymbolHelper.AddOutputDef 4, "oCylinderCapped2", "oCylinderCapped2", 1
        m_oSymbolHelper.AddOutputDef 3, "oCone3", "oCone3", 1
        m_oSymbolHelper.AddOutputDef 4, "oCone4", "oCone4", 1
        m_oSymbolHelper.AddOutputDef 5, "Nozzle1", "Nozzle1", 1
        m_oSymbolHelper.AddOutputDef 6, "Nozzle2", "Nozzle2", 1
    
'**********************************************************************************
'**********************************************************************************
'   END
'**********************************************************************************
'**********************************************************************************
    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "SimplePhysicalAspect Description", 1
    
    Exit Sub
    
ErrorHandler:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

