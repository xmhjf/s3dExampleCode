Option Explicit

Private Const MODULE = "MagneticFlowMeter:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DMagneticFlowMeter"
    m_oSymbolHelper.ClassName = "MagneticFlowMeter"

' Inputs
    
    m_oSymbolHelper.NumInputs = 5
    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "Face to Face", 0.2032
    m_oSymbolHelper.AddInputDef 2, "InstrumentHeight", "Instrument Height", 0.254
    m_oSymbolHelper.AddInputDef 3, "InstrumentLength", "Instrument Length", 0.2032
    m_oSymbolHelper.AddInputDef 4, "InstrumentWidth", "Instrument Width", 0.254
    m_oSymbolHelper.AddInputDef 5, "InstrumentHeight1", "Instrument Height1", 0.254
        
' Outputs
    m_oSymbolHelper.NumOutputs = 9
    m_oSymbolHelper.AddOutputDef 1, "Body", "Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Cylinder1", "Cylinder 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Box1", "Box 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Box2", "Box 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "Cylinder2", "Cylinder 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "ConduitCylinder1", "Conduit Cylinder 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "ConduitCylinder2", "Conduit Cylinder 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "PipingNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "PipingNoz2", "Nozzle 2", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

