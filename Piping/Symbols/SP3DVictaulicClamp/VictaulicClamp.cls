Option Explicit

Private Const MODULE = "VictaulicClamp:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DVictaulicClamp"
    m_oSymbolHelper.ClassName = "VictaulicClamp"

' Inputs
    m_oSymbolHelper.NumInputs = 4
    m_oSymbolHelper.AddInputDef 1, "ClampOuterDiameter", "Clamp Outer Diameter", 0.071
    m_oSymbolHelper.AddInputDef 2, "ClampLength", "Clamp Length", 0.108
    m_oSymbolHelper.AddInputDef 3, "ClampWidth", "Clamp Width", 0.035
    m_oSymbolHelper.AddInputDef 4, "InsulationThickness", "Insulation Thickness", 0.025
        
' Outputs
    m_oSymbolHelper.NumOutputs = 13
    m_oSymbolHelper.AddOutputDef 1, "ClampBodyIns", "Clamp Body Ins", Insulation
    m_oSymbolHelper.AddOutputDef 2, "ClampBodyFront", "Clamp Body Upper", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "ClampBodyRear", "Clamp Body Lower", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "SupFrontTop", "Front Top Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "SupFrontBottom", "Front Bottom Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "SupRearTop", "Rear Top Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "SupRearBottom", "Lower Sup Right", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "LeftTopBolt", "Left Top Bolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "LeftBottomBolt", "Left Bottom Bolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "LeftFrontTopNut", "Left Front Top Nut", SimplePhysical
    m_oSymbolHelper.AddOutputDef 11, "LeftRearTopNut", "Left Rear Top Nut", SimplePhysical
    m_oSymbolHelper.AddOutputDef 12, "LeftFrontBottomNut", "Left Front Bottom Nut", SimplePhysical
    m_oSymbolHelper.AddOutputDef 13, "LeftRearBottomNut", "Left Rear Bottom Nut", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    
    Exit Sub
    
ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

