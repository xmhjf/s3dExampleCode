Option Explicit

Private Const MODULE = "SteamTrapTy5:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DSteamTrapTy5"
    m_oSymbolHelper.ClassName = "SteamTrapTy5"

' Inputs
    m_oSymbolHelper.NumInputs = 8
    m_oSymbolHelper.AddInputDef 1, "PipingSpecialtyHeight", "Piping Specialty Height", 0.124
    m_oSymbolHelper.AddInputDef 2, "PipingSpecialtyLength", "Piping Specialty Length", 0.098
    m_oSymbolHelper.AddInputDef 3, "NozzleOffset", "Nozzle Offset", 0.0222
    m_oSymbolHelper.AddInputDef 4, "PipingSpecialtyWidth", "Piping Specialty Width", 0.137
    m_oSymbolHelper.AddInputDef 5, "NozzleOffset1", "Nozzle Offset 1", 0.0032
    m_oSymbolHelper.AddInputDef 6, "Nozzle1toNozzle2", "Nozzle 1 to Nozzle 2", 0.0698
    m_oSymbolHelper.AddInputDef 7, "NozzleOffset2", "Nozzle Offset 2", 0.0651
    m_oSymbolHelper.AddInputDef 8, "InsulationThickness", "Insulation Thickness", 0.025
    
    ' Outputs
    m_oSymbolHelper.NumOutputs = 8
    m_oSymbolHelper.AddOutputDef 1, "InsTrapBody", "Ins Trap Body", Insulation
    m_oSymbolHelper.AddOutputDef 2, "InsPort1", "Insulation for Port 1", Insulation
    m_oSymbolHelper.AddOutputDef 3, "InsBody1", "Insulation for Body of Port 1", Insulation
    m_oSymbolHelper.AddOutputDef 4, "InsPort2", "Insulation for Port 2", Insulation
    m_oSymbolHelper.AddOutputDef 5, "InsBody2", "Insulation for Body of Port 2", Insulation
    m_oSymbolHelper.AddOutputDef 6, "TrapBody", "Trap Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "PNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "PNoz2", "Nozzle 2", SimplePhysical

    
' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "Physical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    
    Exit Sub
    
ErrorHandler:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
       Err.HelpFile, Err.HelpContext

End Sub


Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

' note that the rest of the code is generated by the Symbol Wizard and should not need to be modified
