Option Explicit

Private Const MODULE = "PressureSwitch"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper

'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DPressureSwitch"
    m_oSymbolHelper.ClassName = "PressureSwitch"


'**********************************************************************************
'**********************************************************************************
'   BEGIN
'**********************************************************************************
'**********************************************************************************

    m_oSymbolHelper.NumInputs = 5
    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "Faceto Face", 0.08
    m_oSymbolHelper.AddInputDef 2, "InstrumentHeight", "Instrument Height", 0.089
    m_oSymbolHelper.AddInputDef 3, "InstrumentWidth", "Instrument Width", 0.06
    m_oSymbolHelper.AddInputDef 4, "InstrumentLength", "Instrument Length", 0.065
    m_oSymbolHelper.AddInputDef 5, "InstrumentHeight1", "Instrument Height 1", 0.06
    
    m_oSymbolHelper.NumOutputs = 6
    m_oSymbolHelper.AddOutputDef 1, "PressureSwitchBotBody", "Pressure Switch Bottom Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "BotBodyConnector", "Pressure Switch Bottom Body Connector", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "PressureSwitchBody", "Pressure Switch Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "PressureSwitchTopBody", "Pressure Switch Top Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "Nozzle1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "Nozzle2", "Nozzle 2", SimplePhysical
    
'**********************************************************************************
'**********************************************************************************
'   END
'**********************************************************************************
'**********************************************************************************
    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "SimplePhysicalAspect Description", 1
    
    Exit Sub
    
ErrorHandler:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

