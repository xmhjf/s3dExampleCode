Option Explicit

Private Const MODULE = "PressureRegulator:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DPressureRegulator"
    m_oSymbolHelper.ClassName = "PressureRegulator"

' Inputs

    m_oSymbolHelper.NumInputs = 4
    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "FacetoFace A", 0.091948
    m_oSymbolHelper.AddInputDef 2, "InstrumentHeight", "InstrumentHeight B", 0.2159
    m_oSymbolHelper.AddInputDef 3, "InstrumentHeight1", "InstrumentHeight1 C", 0.042926
    m_oSymbolHelper.AddInputDef 4, "InstrumentDiameter", "InstrumentDiameter D", 0.038608
           
' Outputs
    m_oSymbolHelper.NumOutputs = 5
    m_oSymbolHelper.AddOutputDef 1, "TheBell", "The Bell", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Nut", "Nut", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "BottomCylinder", "Bottom Cylinder", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "PipingNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "PipingNoz2", "Nozzle 2", SimplePhysical

' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "SimplePhysical", SimplePhysical
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

