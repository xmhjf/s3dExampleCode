Option Explicit

Private Const MODULE = "SteamTrapTy4:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DSteamTrapTy4"
    m_oSymbolHelper.ClassName = "SteamTrapTy4"

' Inputs
    m_oSymbolHelper.NumInputs = 6
    m_oSymbolHelper.AddInputDef 1, "TrapBodyDiameter", "Trap Body Diameter (A)", 0.057
    m_oSymbolHelper.AddInputDef 2, "STHeight", "Steam Trap Height (B)", 0.1
    m_oSymbolHelper.AddInputDef 3, "Face1toCenter", "Face 1 to Center (D)", 0.071
    m_oSymbolHelper.AddInputDef 4, "Face2toCenter", "Face 2 to Center (C)", 0.048
    m_oSymbolHelper.AddInputDef 5, "TrapLength1", "Steam Trap Length 1 (H)", 0.076
    m_oSymbolHelper.AddInputDef 6, "InsulationThickness", "Insulation Thickness", 0.025
        
' Outputs
    m_oSymbolHelper.NumOutputs = 10
    m_oSymbolHelper.AddOutputDef 1, "VertCylinIns", "Vertical Cylinder Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 2, "DomeIns", "Dome Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 3, "PipeinNoz1Ins", "Pipe portion in Nozzle 1 Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 4, "Noz1Ins", "Nozzle 1 Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 5, "PipeinNoz2Ins", "Pipe portion in Nozzle 2 Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 6, "Noz2Ins", "Nozzle 2 Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 7, "VerticalCylin", "Vertical Cylinder", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "Dome", "Dome", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "PNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "PNoz2", "Nozzle 2", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "Physical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
     
    Exit Sub
    
ErrorHandler:
Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

