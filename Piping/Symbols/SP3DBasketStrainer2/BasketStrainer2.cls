Option Explicit

Private Const MODULE = "BasketStrainer2:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DBasketStrainer2"
    m_oSymbolHelper.ClassName = "BasketStrainer2"
    
'' Inputs
    m_oSymbolHelper.NumInputs = 9
    m_oSymbolHelper.AddInputDef 1, "StrDiameter", "Strainer Diameter", 0.165
    m_oSymbolHelper.AddInputDef 2, "EndDiameter", "End Diameter", 0.09
    m_oSymbolHelper.AddInputDef 3, "BasketEndDiameter", "Basket End Diameter", 0.06
    m_oSymbolHelper.AddInputDef 4, "BasketLength", "Basket Length", 0.15
    m_oSymbolHelper.AddInputDef 5, "CollarThickness", "Collar Thickness", 0.002
    m_oSymbolHelper.AddInputDef 6, "ArmLength", "Arm Length", 0.085
    m_oSymbolHelper.AddInputDef 7, "StrWidth", "Strainer Width", 0.033
    m_oSymbolHelper.AddInputDef 8, "CylinderLength", "Cylinder Length", 0.018
    m_oSymbolHelper.AddInputDef 9, "InsulationThickness", "Insulation Thickness", 0.025

' Outputs
    m_oSymbolHelper.NumOutputs = 7
    m_oSymbolHelper.AddOutputDef 1, "CollarIns", "Collar Insulation", Insulation
    m_oSymbolHelper.AddOutputDef 2, "Strainer", "Strainer", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "StrainerEnd", "Strainer End", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Collar", "Collar", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "CollarExtn", "Collar Extension", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "PNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "PNoz2", "Nozzle 2", SimplePhysical

' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "Physical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
     
    Exit Sub
    
ErrorHandler:
Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

