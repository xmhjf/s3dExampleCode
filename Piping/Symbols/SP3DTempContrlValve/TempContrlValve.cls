Option Explicit

Private Const MODULE = "TempContrlValve"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DTempContrlValve"
    m_oSymbolHelper.ClassName = "TempContrlValve"

' Inputs
    
    m_oSymbolHelper.NumInputs = 3
    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "Face to Face", 0.2032
    m_oSymbolHelper.AddInputDef 2, "InstrumentHeight", "Instrument Height", 0.254
    m_oSymbolHelper.AddInputDef 3, "InstrumentHeight1", "Instrument Height 1", 0.254
        
' Outputs
    m_oSymbolHelper.NumOutputs = 11
    m_oSymbolHelper.AddOutputDef 1, "HexagonalNut1", "Hexagonal Nut 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "HexagonalNut2", "Hexagonal Nut 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Cylinder1", "Cylinder 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Cylinder2", "Cylinder 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "Box1", "Box 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "Box2", "Box 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "Cylinder3", "Cylinder 3", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "Cylinder4", "Cylinder 4", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "Cylinder5", "Cylinder 5", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "Nozzle1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 11, "Nozzle2", "Nozzle 2", SimplePhysical

    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

