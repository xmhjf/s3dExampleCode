Option Explicit

Private Const MODULE = "C45UnionElbow:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3D45UnionElbow"
    m_oSymbolHelper.ClassName = "C45UnionElbow"

' Inputs
    
    m_oSymbolHelper.NumInputs = 5
    m_oSymbolHelper.AddInputDef 1, "Face1toCenter", "Face1toCenter E", 0.2032
    m_oSymbolHelper.AddInputDef 2, "Face2toCenter", "Face2toCenter F", 0.254
    m_oSymbolHelper.AddInputDef 3, "UnionDiameter", "UnionDiameter J", 0.12
    m_oSymbolHelper.AddInputDef 4, "TangentLength", "TangentLength K", 0.04445
    m_oSymbolHelper.AddInputDef 5, "InsulationThickness", "Insulation Thickness", 0.025
        
' Outputs
    m_oSymbolHelper.NumOutputs = 12
    m_oSymbolHelper.AddOutputDef 1, "InsulatedTangent1", "Insulated Tangent At Port1", 32
    m_oSymbolHelper.AddOutputDef 2, "InsulatedUnion1", "Insulated Union At Port1", 32
    m_oSymbolHelper.AddOutputDef 3, "InsulatedBody", "Insulated Body", 32
    m_oSymbolHelper.AddOutputDef 4, "InsulatedTangent2", "Insulated Tangent At Port2", 32
    m_oSymbolHelper.AddOutputDef 5, "InsulatedUnion2", "Insulated Union At Port2", 32
    m_oSymbolHelper.AddOutputDef 6, "InsulatedPort1", "Insulated Port1", 32
    m_oSymbolHelper.AddOutputDef 7, "InsulatedPort2", "Insulated Port2", 32
    m_oSymbolHelper.AddOutputDef 8, "Union1", "Union At Port1", 1
    m_oSymbolHelper.AddOutputDef 9, "Elbow", "Elbow", 1
    m_oSymbolHelper.AddOutputDef 10, "Union2", "Union At Port2", 1
    m_oSymbolHelper.AddOutputDef 11, "PipingNoz1", "Nozzle 1", 1
    m_oSymbolHelper.AddOutputDef 12, "PipingNoz2", "Nozzle 2", 1
    
' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "Insulation", "Insulation", 32
    m_oSymbolHelper.AddAspectDef 2, "SimplePhysical", "SimplePhysical", 1
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

