Option Explicit

Private Const MODULE = "ConductivSensor:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DConductivitySensor"
    m_oSymbolHelper.ClassName = "ConductivSensor"

' Inputs
    
    m_oSymbolHelper.NumInputs = 2
    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "Face to Face", 0.2032
    m_oSymbolHelper.AddInputDef 2, "InstrumentHeight", "Instrument Height", 0.254
        
' Outputs
    m_oSymbolHelper.NumOutputs = 19
    m_oSymbolHelper.AddOutputDef 1, "Body", "Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Cylinder1", "Cylinder 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Cylinder2", "Cylinder 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Cylinder3", "Cylinder 3", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "Cylinder4", "Cylinder 4", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "T1Header", "T1 Header", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "T2Header", "T2 Header", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "T3Header", "T3 Header", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "T1Branch", "T1 Branch", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "T1ConnectorBase", "T1 Connector Base", SimplePhysical
    m_oSymbolHelper.AddOutputDef 11, "T1Connector", "T1 Connector ", SimplePhysical
    m_oSymbolHelper.AddOutputDef 12, "T2Branch", "T2 Branch", SimplePhysical
    m_oSymbolHelper.AddOutputDef 13, "T2ConnectorBase", "T2 ConnectorBase", SimplePhysical
    m_oSymbolHelper.AddOutputDef 14, "T2Connector", "T2 Connector ", SimplePhysical
    m_oSymbolHelper.AddOutputDef 15, "T3Branch", "T3 Branch", SimplePhysical
    m_oSymbolHelper.AddOutputDef 16, "T3ConnectorBase", "T3 ConnectorBase", SimplePhysical
    m_oSymbolHelper.AddOutputDef 17, "T3Connector", "T3 Connector ", SimplePhysical
    m_oSymbolHelper.AddOutputDef 18, "PipingNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 19, "PipingNoz2", "Nozzle 2", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

