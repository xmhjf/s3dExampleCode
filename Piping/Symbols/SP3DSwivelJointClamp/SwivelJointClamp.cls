Option Explicit

Private Const MODULE = "SwivelJointClamp:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DSwivelJointClamp"
    m_oSymbolHelper.ClassName = "SwivelJointClamp"

' Inputs
    m_oSymbolHelper.NumInputs = 3
    m_oSymbolHelper.AddInputDef 1, "ClampDiameter", "Clamp Inner Diameter", 0.043
    m_oSymbolHelper.AddInputDef 2, "ClampWidth", "Clamp Width", 0.035
    m_oSymbolHelper.AddInputDef 3, "InsulationThickness", "Insulation Thickness", 0.025

' Outputs
    m_oSymbolHelper.NumOutputs = 5
    m_oSymbolHelper.AddOutputDef 1, "ClampBodyIns", "Clamp Body Ins", Insulation
    m_oSymbolHelper.AddOutputDef 2, "ClampBody", "Clamp Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "WingNutEar", "Wing nut ear", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "WingNutBase", "Wing nut base", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "WingNut", "Wing nut", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    
    Exit Sub
    
ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

