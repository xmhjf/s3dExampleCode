Option Explicit

Private Const MODULE = "ClamponFlowSensor:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()

    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DClamponFlowSensor"
    m_oSymbolHelper.ClassName = "ClamponFlowSensor"

' Inputs
    m_oSymbolHelper.NumInputs = 6
    m_oSymbolHelper.AddInputDef 1, "InstrumentWidth", "Instrument Width", 0.062
    m_oSymbolHelper.AddInputDef 2, "InstrumentHeight", "Instrument Height", 0.111
    m_oSymbolHelper.AddInputDef 3, "InstrumentDiameter", "Instrument Diameter", 0.058
    m_oSymbolHelper.AddInputDef 4, "SensorSpacing", "Sensor Spacing", 0.0005
    m_oSymbolHelper.AddInputDef 5, "ClampSpacing", "Clamp Spacing", 0.872
    m_oSymbolHelper.AddInputDef 6, "ClamptoEnd", "Clamp to End", 0.056

' Outputs
    m_oSymbolHelper.NumOutputs = 8
    m_oSymbolHelper.AddOutputDef 1, "LHSFlowSensor", "Left Hand Side Flow Sensor", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "LHSBaseforFlowSensor", "Left Hand Side base for Flow Sensor", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "LHSClampforFlowSensor", "Left Hand Side Clamp for Flow Sensor", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "LHSClampBolt", "Left Hand Side Clamp Bolt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "RHSFlowSensor", "Right Hand Side Flow Sensor", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "RHSBaseforFlowSensor", "Right Hand Side base for Flow Sensor", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "RHSClampforFlowSensor", "Right Hand Side Clamp for Flow Sensor", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "RHSClampBolt", "Right Hand Side Clamp Bolt", SimplePhysical

' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical

    Exit Sub

ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

