Option Explicit

Private Const MODULE = "ConservationVent:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DConservationVent"
    m_oSymbolHelper.ClassName = "ConservationVent"

' Inputs
    
    m_oSymbolHelper.NumInputs = 4
    m_oSymbolHelper.AddInputDef 1, "Face1toCenter", "Face1toCenter K", 0.12779375
    m_oSymbolHelper.AddInputDef 2, "Face2toCenter", "Face2toCenter L", 0.10795
    m_oSymbolHelper.AddInputDef 3, "InstrumentHeight1", "InstrumentHeight1 J", 0.282575
    m_oSymbolHelper.AddInputDef 4, "InstrumentLength", "InstrumentLength B", 0.2286
           
' Outputs
    m_oSymbolHelper.NumOutputs = 7
    m_oSymbolHelper.AddOutputDef 1, "TopCylinder", "Top Cylinder", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "TopConicalLid", "Top Conical Lid", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "LowerCylinder", "Lower Cylinder", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "ProjTrapezoid", "Projection of Trapezoid", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "LowerConicalLid", "Lower Conical Lid", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "PipingNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "PipingNoz2", "Nozzle 2", SimplePhysical

' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "SimplePhysical", SimplePhysical
    
    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

