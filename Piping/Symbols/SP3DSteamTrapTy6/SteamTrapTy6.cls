Option Explicit

Private Const MODULE = "SteamTrapTy6:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DSteamTrapTy6"
    m_oSymbolHelper.ClassName = "SteamTrapTy6"

' Inputs
    m_oSymbolHelper.NumInputs = 6
    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "Face to Face", 0.095
    m_oSymbolHelper.AddInputDef 2, "PipingSpecialtyLength", "Piping Specialty Length", 0.127
    m_oSymbolHelper.AddInputDef 3, "TrapLength1", "Trap Length", 0.057
    m_oSymbolHelper.AddInputDef 4, "PipingSpecialtyHeight", "Piping Specialty Height", 0.155
    m_oSymbolHelper.AddInputDef 5, "PipingSpecialtyWidth", "Piping Specialty Width", 0.094
    m_oSymbolHelper.AddInputDef 6, "InsulationThickness", "Insulation Thickness", 0.025
    
        
' Outputs
    m_oSymbolHelper.NumOutputs = 9
    m_oSymbolHelper.AddOutputDef 1, "InsTrapBody", "Insulation for Trap  Body", Insulation
    m_oSymbolHelper.AddOutputDef 2, "InsConnectorBody", "Insulation for Connector Body", Insulation
    m_oSymbolHelper.AddOutputDef 3, "InsPort1", "Insulation Port1 Side", Insulation
    m_oSymbolHelper.AddOutputDef 4, "InsBody", "Insulation for pipe body", Insulation
    m_oSymbolHelper.AddOutputDef 5, "InsPort2", "Insulation Port2 Side", Insulation
    m_oSymbolHelper.AddOutputDef 6, "Body", "Body of Trap", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "Connector", "Connector from ports to Trap", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "PNoz1", "Nozzle 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "PNoz2", "Nozzle 2", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "Physical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    
    Exit Sub
    
ErrorHandler:
      Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
       Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

