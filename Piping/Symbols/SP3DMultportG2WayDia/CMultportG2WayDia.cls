Option Explicit

Private Const MODULE = "CMultportG2WayDia:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DMultportG2WayDia"
    m_oSymbolHelper.ClassName = "CMultportG2WayDia"
    
' Inputs
    m_oSymbolHelper.NumInputs = 7
    m_oSymbolHelper.AddInputDef 1, "ValveBodyWidth", "Valve Body Width", 0.1524
    m_oSymbolHelper.AddInputDef 2, "ValveBodyDepth", "Valve Body Depth", 0.1524
    m_oSymbolHelper.AddInputDef 3, "ValveBodyHeight", "Valve Body Height", 0.127
    m_oSymbolHelper.AddInputDef 4, "Face1toCenter", "Face 1 to Center", 0.1143
    m_oSymbolHelper.AddInputDef 5, "Face2toCenter", "Face 2 to Center", 0.1143
    m_oSymbolHelper.AddInputDef 6, "Face3toCenter", "Face 3 to Center", 0.1143
    m_oSymbolHelper.AddInputDef 7, "InsulationThickness", "Insulation Thickness", 0.0254
    
' Outputs
    m_oSymbolHelper.NumOutputs = 10
    m_oSymbolHelper.AddOutputDef 1, "ValveBody", "Valve Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Nozzle1", "Nozzle 1 with length", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Nozzle2", "Nozzle 2 with length", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Nozzle3", "Nozzle 3 with length", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "ValveOperator1", "Port 1 Valve Operator", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "ValveOperator2", "Port 2 Valve Operator", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "InsValveBody", "Ins Valve Body", Insulation
    m_oSymbolHelper.AddOutputDef 8, "InsNozzle1", "Ins Nozzle 1", Insulation
    m_oSymbolHelper.AddOutputDef 9, "InsNozzle2", "Ins Nozzle 2", Insulation
    m_oSymbolHelper.AddOutputDef 10, "InsNozzle3", "Ins Nozzle 3", Insulation

' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    
    Exit Sub
    
ErrorHandler:
  Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
       Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    
    Set m_oSymbolHelper = Nothing

End Sub

