Option Explicit

Private Const MODULE = "ElecContSwitch:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()

    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DElecContSwitch"
    m_oSymbolHelper.ClassName = "ElecContSwitch"

' Inputs
    m_oSymbolHelper.NumInputs = 3
    m_oSymbolHelper.AddInputDef 1, "ElecEquipmentHeight", "Electrical Equipment Height", 0.2032
    m_oSymbolHelper.AddInputDef 2, "ElecEquipmentWidth", "Electrical Equipment Width", 0.2032
    m_oSymbolHelper.AddInputDef 3, "ElecEquipmentLength", "Electrical Equipment Length", 0.2032

' Outputs
    m_oSymbolHelper.NumOutputs = 6
    m_oSymbolHelper.AddOutputDef 1, "DefaultSurface", "Default Surface on the side to be supported", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "ProjectedBody", "Projected Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Surface2", "Surface on other side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "ProjectedKnob", "Projected Knob", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "ConduitPort", "Conduit Port", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "CablePort", "Cable Port", SimplePhysical
    
' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Physical", SimplePhysical

    Exit Sub

ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

