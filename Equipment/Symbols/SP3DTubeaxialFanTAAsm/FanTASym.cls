' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo Errx
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DTubeaxialFanTAAsm"
    m_oSymbolHelper.ClassName = "FanTASym"

    'Inputs
    m_oSymbolHelper.NumInputs = 6

    m_oSymbolHelper.AddInputDef 1, "FacetoFace", "L-Length of Fan", 0.5588
    m_oSymbolHelper.AddInputDef 2, "FlangeDiameter", "A-Flange Diameter", 0.377825
    m_oSymbolHelper.AddInputDef 3, "FanInsideDiameter", "C-Fan Inside Diameter", 0.31115
    m_oSymbolHelper.AddInputDef 4, "BodyThickness", "T-Body Thickness", 0.001905
    m_oSymbolHelper.AddInputDef 5, "NozzleLength", "E-Height of Angle Ring", 0.03175
    m_oSymbolHelper.AddInputDef 6, "FlangeThickness", "F-Thickness of Angle Ring", 0.003175

    'Outputs
    m_oSymbolHelper.NumOutputs = 1
    
    m_oSymbolHelper.AddOutputDef 1, "BodyCylinder", "Body Cylinder", SimplePhysical

    'Aspects
    m_oSymbolHelper.NumAspects = 1
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical

    Exit Sub
Errx:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
        Err.HelpFile, Err.HelpContext
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

