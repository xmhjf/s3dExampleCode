Option Explicit

Private Const MODULE = "SphVesselSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper

' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DE_230SphVesselAsm"
    m_oSymbolHelper.ClassName = "SphVesselSym"

    'Inputs
    m_oSymbolHelper.NumInputs = 9
    
    m_oSymbolHelper.AddInputDef 1, "VesselDiameter", "Vessel Diameter", 2.5
    m_oSymbolHelper.AddInputDef 2, "VesselCenterHeight", "Vessel Center Height", 1.9
    m_oSymbolHelper.AddInputDef 3, "SupportLength", "Support Length", 0.3
    m_oSymbolHelper.AddInputDef 4, "SupportHeight", "Support Height", 1.4
    m_oSymbolHelper.AddInputDef 5, "SupportThickness", "Support Thickness", 0.1
    m_oSymbolHelper.AddInputDef 6, "NumberOfSupports", "Number Of Supports", 3
    m_oSymbolHelper.AddInputDef 7, "SupportAngularLocation", "Support Angular Location", 25
    m_oSymbolHelper.AddInputDef 8, "SupportRadialLocation", "Support Radial Location", 0.8
    m_oSymbolHelper.AddInputDef 9, "InsulationThickness", "Insulation Thickness", 0.025
    
    'Outputs
    m_oSymbolHelper.NumOutputs = 10
    
    m_oSymbolHelper.AddOutputDef 1, "InsulatedBody", "Insulated Body", Insulation
    m_oSymbolHelper.AddOutputDef 2, "SphericalVesselCompControlPoint", "Control Point of Spherical Vessel Component", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 3, "Vessel", "Vessel", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "EqpFoundationPort", "Foundation Port under support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "Support_", "Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "Edge_", "Edge", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "Point1_", "Point1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "Point2_", "Point2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "Surface_", "Surface", SimplePhysical
    
   'Aspects
    m_oSymbolHelper.NumAspects = 3
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    m_oSymbolHelper.AddAspectDef 3, "RefGeometry", "ReferenceGeometry", ReferenceGeometry

    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

