    m_oSymbolHelper.AddInputDef 2, "ElecEquipmentWidth", "Width of ElecEquipment", 0.8
    m_oSymbolHelper.AddInputDef 3, "ElecEquipmentLength", "Length of ElecEquipment", 0.8
