    m_oSymbolHelper.AddOutputDef 3, "ConduitPort1", "ConduitPort1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "CablePort1", "CablePort1", SimplePhysical
    
    m_oSymbolHelper.AddOutputDef 5, "CenterPos1", "FaceCenter Position1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "CenterPos2", "FaceCenter Position2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "CenterPos3", "FaceCenter Position3", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "CenterPos4", "FaceCenter Position4", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "CenterPos5", "FaceCenter Position5", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "CenterPos6", "FaceCenter Position6", SimplePhysical
    
