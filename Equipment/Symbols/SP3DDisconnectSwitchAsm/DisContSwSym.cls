    m_oSymbolHelper.AddOutputDef 12, "Edge2", "Edge 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 13, "Edge3", "Edge 3", SimplePhysical
    m_oSymbolHelper.AddOutputDef 14, "Edge4", "Edge 4", SimplePhysical
    m_oSymbolHelper.AddOutputDef 15, "Edge5", "Edge 5", SimplePhysical
    m_oSymbolHelper.AddOutputDef 16, "Edge6", "Edge 6", SimplePhysical

    m_oSymbolHelper.AddOutputDef 17, "Edge7", "Edge 7", SimplePhysical
