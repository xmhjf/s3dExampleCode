Private m_oSymbolHelper As IJSymbolHelper
    
' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices


Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DFallFilmSTExAsm"
    m_oSymbolHelper.ClassName = "FallingFShTubeExSym"
    
'' Inputs
    m_oSymbolHelper.NumInputs = 5
    m_oSymbolHelper.AddInputDef 1, "VesselDiameter", "Exchanger Outer Diameter", 1.4986
    m_oSymbolHelper.AddInputDef 2, "VesselHeight", "Front Head Height", 2.286
    m_oSymbolHelper.AddInputDef 3, "VesselHeight1", "Exchanger Shell Height", 7.30885
    m_oSymbolHelper.AddInputDef 4, "DomeHeight", "Front Head Bonnet Height", 0.381
    m_oSymbolHelper.AddInputDef 5, "VesselStartPoint", "Control Point to Dome Center point or Tan point of front head", 0.762

' Outputs
    m_oSymbolHelper.NumOutputs = 13
    m_oSymbolHelper.AddOutputDef 1, "FrontHeadBonnet", "Elliptical Front Head Bonnet Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "FrontHeadBodyCyl", "Cylindrical Front Head Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "FrontHeadFlangeTop", "Top Flange on Front head side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "FrontHeadFlangeBottom", "Bottom Flange on Front head side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "ShellBodyCyl", "Cylindrical Exchanger Shell Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "RearHeadFlangeTop", "Top Flange on Rear Head side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "RearHeadFlangeBottom", "Bottom Flange on Rear Head side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "RearHeadMiterBottom15degBody", "Rear Head Miter Bottom 15 degree portion", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "RearHeadMiterBottom30degBody", "Rear Head Miter Bottom 30 degree portion", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "RearHeadMiterTop30degBody", "Rear Head Miter Top 30 degree portion", SimplePhysical
    m_oSymbolHelper.AddOutputDef 11, "RearHeadMiterTop15degBody", "Rear Head Miter Top 15 degree portion", SimplePhysical
    m_oSymbolHelper.AddOutputDef 12, "RearHeadMiterTopCyl", "Rear Head Miter Top Cylinder portion", SimplePhysical
    m_oSymbolHelper.AddOutputDef 13, "ControlPoint", "Control Point on Vessel Axis", ReferenceGeometry
    

' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry
    
    Exit Sub
    
ErrorHandler:
Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

