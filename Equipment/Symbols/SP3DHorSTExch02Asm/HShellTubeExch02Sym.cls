' Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices


Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DHorSTExch02Asm"
    m_oSymbolHelper.ClassName = "HShellTubeExch02Sym"
    
' Inputs
    m_oSymbolHelper.NumInputs = 10
    m_oSymbolHelper.AddInputDef 1, "VesselDiameter", "Exchanger Diameter", 1.803
    m_oSymbolHelper.AddInputDef 2, "VesselLength", "Exchanger  Length", 9.906
     m_oSymbolHelper.AddInputDef 3, "VesselTantoTan", "Exchanger  Tangent to Tanget Length", 8.9662
    m_oSymbolHelper.AddInputDef 4, "VesselLength1", "Front Head Length", 0.9652
    m_oSymbolHelper.AddInputDef 5, "VesselStartPoint", "Distance of Control Point from Tangent", 0.4572
    m_oSymbolHelper.AddInputDef 6, "SupportLength", "Support Thickness", 0.305
    m_oSymbolHelper.AddInputDef 7, "SupportWidth", "Support Width", 1.6
    m_oSymbolHelper.AddInputDef 8, "SupportHeight", "Support Height ", 1.372
    m_oSymbolHelper.AddInputDef 9, "FirstSupportLocation", "First Support Location ", 1.168
    m_oSymbolHelper.AddInputDef 10, "SecondSupportLocation", "Second Support Location", 6.96
    

' Outputs
    m_oSymbolHelper.NumOutputs = 10
    m_oSymbolHelper.AddOutputDef 1, "FrontHeadBonnet", "Elliptical Front Head Bonnet Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "FrontHeadBodyCyl", "Cylindrical Front Head Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "FrontHeadFlangeTop", "Top Flange on Front head side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "FrontHeadFlangeBottom", "Bottom Flange on Front head side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "ShellBodyCyl", "Cylindrical Exchanger Shell Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "RearHeadBonnet", "Elliptical Front Head Bonnet Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "ExchangerSupport1", "Exchanger support1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "ExchangerSupport2", "Exchanger support2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "ControlPoint", "ControlPoint at origin", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 10, "DefaultSurface", "Default Surface  at Support", ReferenceGeometry
    
    

' Aspects
    m_oSymbolHelper.NumAspects = 2
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "PipingAspect Description", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "ReferenceGeometry", "ReferenceGeometry", ReferenceGeometry
    
    Exit Sub
    
ErrorHandler:
   Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

