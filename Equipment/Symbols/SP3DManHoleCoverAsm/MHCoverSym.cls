Option Explicit

Private Const MODULE = "MHCoverSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
  
'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DManHoleCoverAsm"
    m_oSymbolHelper.ClassName = "MHCoverSym"

    Dim EquipmentHole As Long
    EquipmentHole = 524288 'Aspect No 19 for equipment holes

    'Inputs
    m_oSymbolHelper.NumInputs = 8
    
    m_oSymbolHelper.AddInputDef 1, "CoverLength", "Cover Length", 1.647825
    m_oSymbolHelper.AddInputDef 2, "CoverWidth", "Cover Width", 1.495425
    m_oSymbolHelper.AddInputDef 3, "CoverThickness", "Cover Thickness", 0.0127
    m_oSymbolHelper.AddInputDef 4, "CoverHeight", "Cover Height", 0.0127
    m_oSymbolHelper.AddInputDef 5, "DistBetBoltHoles", "Space between Bolt Holes", 0.1778
    m_oSymbolHelper.AddInputDef 6, "HoleLength", "Hole Length", 1.698625
    m_oSymbolHelper.AddInputDef 7, "HoleWidth", "Hole Width", 1.524
    m_oSymbolHelper.AddInputDef 8, "HoleThickness", "Hole Thickness", 1.524
    
    'Outputs
    m_oSymbolHelper.NumOutputs = 7
    
    m_oSymbolHelper.AddOutputDef 1, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "InnerPlate", "Inner Plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Bolt_", "Bolts", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "OuterPlate_", "Outer Plate", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "BottomPart_", "Bottom Part", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "Handle_", "Handles", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "Hole", "Hole", EquipmentHole
    
    'Aspects
    m_oSymbolHelper.NumAspects = 2
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "MHCoverHole", "Man Hole Cover Hole", EquipmentHole
    
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

