Option Explicit

Private Const MODULE = "LightFixSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
  
'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DFluoLightFixWORAsm"
    m_oSymbolHelper.ClassName = "LightFixSym"

    'Inputs
    m_oSymbolHelper.NumInputs = 3
    
    m_oSymbolHelper.AddInputDef 1, "FixtureLength", "Fixture Length", 1.2
    m_oSymbolHelper.AddInputDef 2, "FixtureWidth", "Fixture Width", 0.325
    m_oSymbolHelper.AddInputDef 3, "FixtureHeight", "Fixture Height", 0.125

    'Outputs
    m_oSymbolHelper.NumOutputs = 6
    
    m_oSymbolHelper.AddOutputDef 1, "FixtureBody", "Body of the fixture", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "ConduitPort1", "ConduitPort 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "ConduitPort2", "ConduitPort 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "CablePort1", "CablePort 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "CablePort2", "CablePort 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "DefaultSurface", "Default Surface", SimplePhysical

    'Aspects
    m_oSymbolHelper.NumAspects = 1
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

