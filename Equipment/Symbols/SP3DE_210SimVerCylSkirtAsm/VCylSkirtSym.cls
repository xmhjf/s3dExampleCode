Option Explicit

Private Const MODULE = "VCylSkirtSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
  
'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DE_210SimVerCylSkirtAsm"
    m_oSymbolHelper.ClassName = "VCylSkirtSym"

    'Inputs
    m_oSymbolHelper.NumInputs = 16
    
    m_oSymbolHelper.AddInputDef 1, "End1Type", "Vessel Front Head Type", 1
    m_oSymbolHelper.AddInputDef 2, "End2Type", "Vessel Rear Head Type", 1
    m_oSymbolHelper.AddInputDef 3, "VesselTantoTan", "Vessel Tangent to Tangent", 8#
    m_oSymbolHelper.AddInputDef 4, "VesselDiameter", "Vessel Diameter", 2.1
    m_oSymbolHelper.AddInputDef 5, "SkirtTopDiameter", "Support Top Diameter", 2.1
    m_oSymbolHelper.AddInputDef 6, "SkirtBottomDiameter", "Support Bottom Diameter", 3.5
    m_oSymbolHelper.AddInputDef 7, "SkirtHeight", "Skirt Height", 2.7
    m_oSymbolHelper.AddInputDef 8, "SupportReferencePoint", "Support Reference Point", 0.1
    m_oSymbolHelper.AddInputDef 9, "EndHeadConeTopDiameter", "End Head Cone Top Diameter", 1.4
    m_oSymbolHelper.AddInputDef 10, "EndHeadConeHeight", "End Head Cone Height", 1#
    m_oSymbolHelper.AddInputDef 11, "EndHeadKnuckleRadius", "End Head Knuckle Radius", 1#
    m_oSymbolHelper.AddInputDef 12, "EndHeadDomeradius", "End Head Dome Radius", 1.2
    m_oSymbolHelper.AddInputDef 13, "EndHeadFlangedThick1", "End Head Flanged Thickness 1", 0.2
    m_oSymbolHelper.AddInputDef 14, "EndHeadFlangedThick2", "End Head Flanged Thickness 2", 0.2
    m_oSymbolHelper.AddInputDef 15, "EndHeadSphericalRadius", "End Head Spherical Radius", 1.2
    m_oSymbolHelper.AddInputDef 16, "InsulationThickness", "Insulation Thickness", 0.025
   
    'Outputs
    m_oSymbolHelper.NumOutputs = 6
    
    m_oSymbolHelper.AddOutputDef 1, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Vessel", "Vessel", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "SupportSkirt", "Support Skirt", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "EqpFoundatioPort1", "Foundation Port 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "VesselwithSkirtEqpControlPoint", "Control Point of Vessel with Skirt", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 6, "InsulatedVessel", "Insulated Vessel", Insulation
    
    'Aspects
    m_oSymbolHelper.NumAspects = 3
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    m_oSymbolHelper.AddAspectDef 3, "RefGeometry", "ReferenceGeometry", ReferenceGeometry
   
    Exit Sub
ErrorHandler:
  ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

'Note that the rest of the code is generated by the Symbol Wizard and should not need to be modified
