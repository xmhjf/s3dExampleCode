Option Explicit

Private Const MODULE = "VertVesselEndsSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper

'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()

    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler

    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DVertVesselEndsAsm"
    m_oSymbolHelper.ClassName = "VertVesselEndsSym"

    'Inputs
    m_oSymbolHelper.NumInputs = 12
    
    m_oSymbolHelper.AddInputDef 1, "VesselTantoTan", "Vessel Tangent to Tangent", 3
    m_oSymbolHelper.AddInputDef 2, "VesselDiameter", "Vessel Diameter", 0.5
    m_oSymbolHelper.AddInputDef 3, "InsulationThickness", "InsulationThickness", 0.025
    m_oSymbolHelper.AddInputDef 4, "End1Type", "End1 Type", 1
    m_oSymbolHelper.AddInputDef 5, "End2Type", "End2 Type", 1
    m_oSymbolHelper.AddInputDef 6, "EndHeadConeTopDiameter", "End Head Cone Top Diameter", 0.1
    m_oSymbolHelper.AddInputDef 7, "EndHeadConeHeight", "End Head Cone Height", 0.2
    m_oSymbolHelper.AddInputDef 8, "EndHeadKnuckleRadius", "End Head Knuckle Radius", 0.2
    m_oSymbolHelper.AddInputDef 9, "EndHeadDomeradius", "End Head Dome radius", 0.3
    m_oSymbolHelper.AddInputDef 10, "EndHeadFlangedThick1", "End Head Flanged Thickness 1", 0.2
    m_oSymbolHelper.AddInputDef 11, "EndHeadFlangedThick2", "End Head Flanged Thickness 2", 0.2
    m_oSymbolHelper.AddInputDef 12, "EndHeadSphericalRadius", "End Head Spherical Radius", 0.3

    'Outputs
    m_oSymbolHelper.NumOutputs = 3
    
    m_oSymbolHelper.AddOutputDef 1, "ObjVertVessel", "Vertical Vessel", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "ObjVertVesselIns", "Insulated Vertical Vessel", Insulation
    m_oSymbolHelper.AddOutputDef 3, "VerticalVesselEndsControlPoint", "Control Point of Vertical Vessel Ends", ReferenceGeometry
    'Aspects
    m_oSymbolHelper.NumAspects = 3
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "Insulation", "Insulation", Insulation
    m_oSymbolHelper.AddAspectDef 3, "RefGeometry", "ReferenceGeometry", ReferenceGeometry
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

'Note that the rest of the code is generated by the Symbol Wizard and should not need to be modified
