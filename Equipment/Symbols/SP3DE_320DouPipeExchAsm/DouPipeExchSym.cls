    m_oSymbolHelper.AddInputDef 2, "PipeEndFromPP", "Pipe End from Placement Point", 0.2
    m_oSymbolHelper.AddInputDef 3, "PipeCentoCen", "Pipe Center to Center", 0.4
    m_oSymbolHelper.AddInputDef 4, "PipeDiameter", "Diameter of Pipe", 0.1
    m_oSymbolHelper.AddInputDef 5, "BlockLength", "Block Length", 0.2
    m_oSymbolHelper.AddInputDef 6, "BlockHeight", "Block Height", 0.8
    m_oSymbolHelper.AddInputDef 7, "BlockWidth", "Block Width", 0.2
    m_oSymbolHelper.AddInputDef 8, "BundlePullingLength", "Bundle Pulling Length", 1.2
    m_oSymbolHelper.AddInputDef 9, "Support1CenFromPP", "Support 1 Center from Placement Point", 0.2
    m_oSymbolHelper.AddInputDef 10, "SupportCentoCen", "Support Center to Center", 0.4
    m_oSymbolHelper.AddInputDef 11, "SupportHeight", "Height of Support", 1.2
    m_oSymbolHelper.AddInputDef 12, "SupportThickness", "Thickness of Support", 0.1
    m_oSymbolHelper.AddInputDef 13, "SupportWidth", "Width of Support", 0.4
    m_oSymbolHelper.AddInputDef 14, "XBoltHole", "Hole Bolt X Location", 0.2
    m_oSymbolHelper.AddInputDef 15, "YBoltHole", "Hole Bolt Y Location", 0.05

    'Outputs
    m_oSymbolHelper.NumOutputs = 11
    
    m_oSymbolHelper.AddOutputDef 1, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "Pipe1", "Pipe 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "Pipe2", "Pipe 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Block", "Block", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "Edges_", "Edges of Block", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "Points_", "Points on each face of Block", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "Supports_", "Support 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "EqpFoundationPort1", "EqpFoundation Port1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "EqpFoundationPort2", "EqpFoundation Port1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "DouPipeExchangerControlPoint", "Control Point of Double Pipe Exchanger", ReferenceGeometry
    m_oSymbolHelper.AddOutputDef 11, "BundlePulling", "BundlePulling", Maintenance

    'Aspects
    m_oSymbolHelper.NumAspects = 3
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    m_oSymbolHelper.AddAspectDef 2, "RefGeometry", "ReferenceGeometry", ReferenceGeometry
    m_oSymbolHelper.AddAspectDef 3, "Maintenance", "Maintenance", Maintenance
    
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
