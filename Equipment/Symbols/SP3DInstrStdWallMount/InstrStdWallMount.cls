    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DInstrStdWallMount"
    m_oSymbolHelper.ClassName = "InstrStdWallMount"

' Inputs
    m_oSymbolHelper.NumInputs = 1
    m_oSymbolHelper.AddInputDef 1, "InstrumentStandHeight", "InstrumentStandHeight", 1.3716
    
' Outputs
    m_oSymbolHelper.NumOutputs = 4
    m_oSymbolHelper.AddOutputDef 1, "BasePlate", "Base Plate of Stand", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "ColumnBody1", "Column Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "ColumnBody2", "Column Body", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "EqpFoundationPort", "Foundation Port", SimplePhysical
    

' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "PipingAspect Description", SimplePhysical
    
    Exit Sub
    
ErrorHandler:
Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub

