Option Explicit

Private Const MODULE = "BTARSym:"  'Used for error messages
Private m_oSymbolHelper As IJSymbolHelper
  
'Declaration of the User Symbol Services interface
Implements IJDUserSymbolServices

Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DLightingFixtureBTARAsm"
    m_oSymbolHelper.ClassName = "BTARSym"

    'Inputs
    m_oSymbolHelper.NumInputs = 6
    
    m_oSymbolHelper.AddInputDef 1, "TankDia", "Tank Diameter", 0.3449
    m_oSymbolHelper.AddInputDef 2, "TankLength", "Tank Length", 0.1044
    m_oSymbolHelper.AddInputDef 3, "GuardLength", "Guard Length", 0.2068
    m_oSymbolHelper.AddInputDef 4, "ReflectorBottomDia", "Reflector Bottom Diameter", 0.3746
    m_oSymbolHelper.AddInputDef 5, "ReflectorLength1", "Reflector Length 1", 0.0668
    m_oSymbolHelper.AddInputDef 6, "ReflectorLength2", "Reflector Length 2", 0.2738
    
    'Outputs
    m_oSymbolHelper.NumOutputs = 7
    
    m_oSymbolHelper.AddOutputDef 1, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "BallastTank", "Ballast Tank", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "BallastTankSupport", "Ballast Tank Support", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "Reflector", "Reflector", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "Guard_", "Guard", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "ConduitPort", "Conduit Port", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "CablePort", "Cable Port", SimplePhysical
    
    'Aspects
    m_oSymbolHelper.NumAspects = 1
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

