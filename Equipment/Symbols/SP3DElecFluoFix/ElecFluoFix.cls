Option Explicit

Private Const MODULE = "ElecFluoFix:"  'Used for error messages

Private m_oSymbolHelper As IJSymbolHelper
    
'Declaration of the User Symbol Services interface

Implements IJDUserSymbolServices


Private Sub Class_Initialize()
    
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    Dim index As Long
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DElecFluoFix"
    m_oSymbolHelper.ClassName = "ElecFluoFix"
    
'inputs
    m_oSymbolHelper.NumInputs = 3
    m_oSymbolHelper.AddInputDef 1, "ElecEquipmentLength", "Length of the Fixture", 1.2
    m_oSymbolHelper.AddInputDef 2, "ElecEquipmentHeight", "Depth of the Fixture", 0.16
    m_oSymbolHelper.AddInputDef 3, "ElecEquipmentWidth", "Width of the Fixture", 0.32
    
'Outputs
    m_oSymbolHelper.NumOutputs = 9
    m_oSymbolHelper.AddOutputDef 1, "TopCover", "Top Cover of the fixture", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "SideCoverRight", "The Side Cover at the Right Hand Side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "SideCoverLeft", "The Side Cover at the Left Hand Side", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "FrontCover", "Front Parabolic Cover", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "RearCover", "Rear Parabolic Cover", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "FluorescentTube1", "Flourescent Tube", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "FluorescentTube2", "Flourescent Tube", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "FluorescentTube3", "Flourescent Tube", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "ConduitPort", "Wire-Inlet", SimplePhysical

' Aspects
    m_oSymbolHelper.NumAspects = 1
    m_oSymbolHelper.AddAspectDef 1, "SimplePhysical", "Wallpack Aspect Description", SimplePhysical


Exit Sub
    
ErrorHandler:
    Err.Raise Err.Number, Err.Source & " " & METHOD, Err.Description, _
    Err.HelpFile, Err.HelpContext

End Sub
    
Private Sub Class_Terminate()
    Set m_oSymbolHelper = Nothing
End Sub

