Private Sub Class_Initialize()
    Const METHOD = "Class_Initialize:"
    On Error GoTo ErrorHandler
    
    Set m_oSymbolHelper = New SymbolServices
    m_oSymbolHelper.ProjectName = "SP3DLightingFixtureBHAsmTy1"
    m_oSymbolHelper.ClassName = "BHTy1Sym"

    'Inputs
    m_oSymbolHelper.NumInputs = 4
    
    m_oSymbolHelper.AddInputDef 1, "HousingDia", "Housing Diameter", 0.3449
    m_oSymbolHelper.AddInputDef 2, "HousingLength", "Housing Length", 0.2088
    m_oSymbolHelper.AddInputDef 3, "GuardLength", "Guard Length", 0.2992
    m_oSymbolHelper.AddInputDef 4, "GuardDia", "Guard Diameter", 0.2451
    
    'Outputs
    m_oSymbolHelper.NumOutputs = 13
    
    m_oSymbolHelper.AddOutputDef 1, "DefaultSurface", "Default Surface", SimplePhysical
    m_oSymbolHelper.AddOutputDef 2, "BallastHousing", "Ballast Housing", SimplePhysical
    m_oSymbolHelper.AddOutputDef 3, "GuardRib1", "Guard Rib 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 4, "GuardRib2", "Guard Rib 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 5, "GuardRib3", "Guard Rib 3", SimplePhysical
    m_oSymbolHelper.AddOutputDef 6, "GuardRib4", "Guard Rib 4", SimplePhysical
    m_oSymbolHelper.AddOutputDef 7, "GuardRib5", "Guard Rib 5", SimplePhysical
    m_oSymbolHelper.AddOutputDef 8, "GuardRib6", "Guard Rib 6", SimplePhysical
    m_oSymbolHelper.AddOutputDef 9, "ConduitPort", "Conduit Port", SimplePhysical
    m_oSymbolHelper.AddOutputDef 10, "CablePort", "Cable Port", SimplePhysical
    m_oSymbolHelper.AddOutputDef 11, "Guard1", "Guard Torus 1", SimplePhysical
    m_oSymbolHelper.AddOutputDef 12, "Guard2", "Guard Torus 2", SimplePhysical
    m_oSymbolHelper.AddOutputDef 13, "Guard3", "Guard Torus 3", SimplePhysical
    
    'Aspects
    m_oSymbolHelper.NumAspects = 1
    
    m_oSymbolHelper.AddAspectDef 1, "Physical", "Physical", SimplePhysical
    
    Exit Sub
ErrorHandler:
    ReportUnanticipatedError MODULE, METHOD
End Sub

Private Sub Class_Terminate()
