    m_oSymbolHelper.AddInputDef 14, "XBoltHole", "HE-Hole Bolt X Location", 0.508
    m_oSymbolHelper.AddInputDef 15, "YBoltHole", "HF-Hole Bolt Y Location", 0.508
    
    'Outputs
    m_oSymbolHelper.NumOutputs = 11
    
    m_oSymbolHelper.AddOutputDef 1, "CircularEdge", "Circular Edge", SimplePhysical
